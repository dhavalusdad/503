{
  "name": "telehealth",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "lint": "eslint . --max-warnings=0",
