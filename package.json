    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve": "if [ -d dist ]; then npx serve dist; fi",
    "prepare": "husky",
    "preview:watch": "vite build --watch & vite preview",
    "postinstall": "mkdir -p public/twilio-video-processors && cp -r node_modules/@twilio/video-processors/dist/build/* public/twilio-video-processors/"
